{
  "name": "gbooks",
  "version": "1.0.0",
  "description": "Search and and save books from the google books api",
  "main": "server.js",
  "engine": {
    "node": "12.18.0",
    "npm": "6.14.4"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agnide4/gbooks.git"
  },
  "keywords": [
    "MERN"
  ],
  "author": "Samir the Learner",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/agnide4/gbooks/issues"
  },
  "homepage": "https://github.com/agnide4/gbooks#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.10.8",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4"
  }
}
